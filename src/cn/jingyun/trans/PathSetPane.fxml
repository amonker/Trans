<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" alignment="CENTER_LEFT" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label text="FFmpeg Path:">
      <VBox.margin>
        <Insets left="5.0" top="5.0" />
      </VBox.margin>
    </Label>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <TextField fx:id="txtFFmpegPath" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Button fx:id="btnFFmpeg" mnemonicParsing="false" onAction="#onBtnChoiceFFmpegPathClicked" text="" />
      </children>
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" fx:id="x1" />
      </VBox.margin>
    </HBox>
    <Label text="MP4Box Path:">
      <VBox.margin>
        <Insets left="5.0" top="3.0" />
      </VBox.margin>
    </Label>
    <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.margin="$x1">
      <children>
        <TextField fx:id="txtMP4BoxPath" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Button id="btnMP4" fx:id="btnMP4Box" mnemonicParsing="false" onAction="#onBtnChoiceMP4BoxPathClicked" text="" />
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
</fx:root>
