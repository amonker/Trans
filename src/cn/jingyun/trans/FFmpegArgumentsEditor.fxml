<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" xmlns:fx="http://javafx.com/fxml">
  <children>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label alignment="CENTER" contentDisplay="LEFT" text="名称">
          <HBox.margin>
            <Insets bottom="2.0" left="1.0" right="40.0" top="3.0" />
          </HBox.margin>
        </Label>
        <TextField fx:id="txtName" prefWidth="-1.0" />
      </children>
      <VBox.margin>
        <Insets bottom="4.0" left="8.0" right="8.0" top="12.0" />
      </VBox.margin>
    </HBox>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label text="文件扩展名">
          <HBox.margin>
            <Insets bottom="2.0" left="1.0" right="5.0" top="3.0" />
          </HBox.margin>
        </Label>
        <TextField fx:id="txtFileExtension" prefWidth="-1.0" />
      </children>
      <VBox.margin>
        <Insets bottom="4.0" left="8.0" right="8.0" top="6.0" />
      </VBox.margin>
    </HBox>
    <Label text="命令行内容">
      <VBox.margin>
        <Insets bottom="2.0" left="8.0" top="12.0" />
      </VBox.margin>
    </Label>
    <TextArea fx:id="txtArguments" prefWidth="200.0" wrapText="true">
      <VBox.margin>
        <Insets bottom="8.0" left="8.0" right="8.0" top="1.0" />
      </VBox.margin>
    </TextArea>
  </children>
</fx:root>
